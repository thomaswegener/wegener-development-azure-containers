{
  "name": "piratehusky",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=20.17.0"
  },
  "scripts": {
    "dev": "concurrently -k \"npm:server\" \"vite\"",
    "build": "vite build && vite build --ssr src/entry-server.jsx --outDir dist-ssr && node scripts/prerender.mjs",
    "server": "cross-env NODE_ENV=development PORT=4000 node server/index.js",
