    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.3.8",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "express": "^4.22.1",
    "i18next": "^25.6.2",
    "jsonwebtoken": "^9.0.3",
    "react": "^19.2.0",
    "react-bootstrap-icons": "^1.11.6",
    "react-cookie-consent": "^9.0.0",
    "react-dom": "^19.2.0",
    "react-i18next": "^15.7.4",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.13.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^4.7.0",
    "concurrently": "^9.1.2",
    "eslint": "^9.39.1",
