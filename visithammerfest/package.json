    "@prisma/client": "^5.20.0",
    "argon2": "^0.40.3",
    "dotenv": "^16.4.5",
    "fastify": "^4.28.1",
    "mysql2": "^3.11.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.27.0",
    "sharp": "^0.34.5"
