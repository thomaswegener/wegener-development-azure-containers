# Copy to .env and edit as needed
JOB_SIGNING_SECRET=dev-secret-change-me
OBSIDIAN_VAULT_PATH=/absolute/path/to/your/vault

# If using docker-compose.vault.yml to sync vault from GitHub:
VAULT_GIT_REPO_URL=
VAULT_GIT_BRANCH=main
VAULT_GIT_USERNAME=
VAULT_GIT_TOKEN=
VAULT_SYNC_INTERVAL=300

# LLM / embeddings (optional for MVP; if unset, chat echoes and RAG uses keyword search)
OPENAI_API_KEY=
OPENAI_BASE_URL=
OPENAI_MODEL=gpt-4.1-mini
EMBEDDING_MODEL=text-embedding-3-small
