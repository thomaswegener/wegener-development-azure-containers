    "@prisma/client": "^5.12.0",
    "dotenv": "^16.4.5",
    "fastify": "^4.26.2",
    "nodemailer": "^6.9.13"
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "@types/nodemailer": "^6.4.15",
    "prisma": "^5.12.0",
    "tsx": "^4.7.3",
    "typescript": "^5.4.5"
  }
